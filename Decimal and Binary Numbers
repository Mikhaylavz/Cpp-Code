#include <iostream>
using namespace std;
int main()
{
	int a[10], number, i;
	cout << "Enter the number to convert: ";
	cin >> number;
	for (i = 0; number > 0; i++)
	{
		a[i] = number % 2;
		number = number / 2;
	}
	cout << "Binary of the given number= ";
	for (i = i - 1; i >= 0; i--)
	{
		cout << a[i];
	}
}
